    <script>
        const users = [
            { sno: 1, studentid: 12345, name: "Isha", active: true },
            { sno: 2, studentid: 123345, name: "Aarti", active: false },
        ];
        const userTable = document.getElementById('user-table');
        function updateUserTable() {
            userTable.innerHTML = '';
            users.forEach(user => {
                const row = document.createElement('tr');
                const snoCell = document.createElement('td');
                snoCell.textContent = user.sno;
                row.appendChild(snoCell);
                const studentIdCell = document.createElement('td');
                studentIdCell.textContent = user.studentid;
                row.appendChild(studentIdCell);
                const nameCell = document.createElement('td');
                nameCell.textContent = user.name;
                row.appendChild(nameCell);
                const statusCell = document.createElement('td');
                statusCell.textContent = user.active ? 'Active' : 'Inactive';
                statusCell.classList.add(user.active ? 'active' : 'inactive');
                row.appendChild(statusCell);
                const actionCell = document.createElement('td');
                actionCell.classList.add('action-cell');

                const editButton = document.createElement('button');
                editButton.classList.add('edit-btn');
                editButton.onclick = () => editUser(user);
                const editIcon = document.createElement('button');
                editIcon.alt = 'Edit';
                editIcon.width = 16;
                editIcon.height = 16;
                editButton.appendChild(document.createTextNode('Edit'));
                editButton.appendChild(editIcon);
                actionCell.appendChild(editButton);
                
                row.appendChild(actionCell);
                userTable.appendChild(row);
            });
        }
        function editUser(user) {
            alert(`Editing user: ${user.name}`);
        }
        function infoUser(user) {
            alert(`Information for user: ${user.name}`);
        }
        updateUserTable();

        
        document.addEventListener('DOMContentLoaded', (event) => {
            const switchMode = document.getElementById('switch-mode');
            const isDarkMode = localStorage.getItem('dark-mode') === 'true';
            document.body.classList.toggle('dark-mode', isDarkMode);
            switchMode.checked = isDarkMode;
        });

        
        document.getElementById('switch-mode').addEventListener('change', function() {
            const isDarkMode = this.checked;
            document.body.classList.toggle('dark-mode', isDarkMode);
            
            localStorage.setItem('dark-mode', isDarkMode);
        });

